{
  "expo": {
    "name": "clientside",
    "slug": "clientside",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#004080"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-location"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#004080"
      },
      "package": "com.jasonaddy.clientside",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a7f7cd75-b049-4974-8b9a-b479e9de80d3"
      }
    },
    "scheme": "clientside",
    "owner": "jasonaddy"
  }
}
